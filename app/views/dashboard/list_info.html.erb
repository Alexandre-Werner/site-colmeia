  <h1 class="text-2xl text-gray-700 font-bold">Editar Informações da home page:</h1>
  <div class="ml-4 mt-10">
    <h2 class="text-gray-700 font-bold">Linker</h1>
    <div class="mt-3 Post-it">
      <div class="flex flex-col">
        <div class="-my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
          <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
            <div class="shadow overflow-hidden border-b border-gray-200 sm:rounded-lg">
              <table class="min-w-full divide-y divide-gray-200">
                <thead class="bg-gray-50">
                  <tr>
                    <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                      Texto
                    </th>
                    <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                      Botão
                    </th>
                    <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                      Link
                    </th>
                    <th scope="col" class="relative px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                    <span class="sr-only"></span>
                    </th>
                  </tr>
                </thead>
                <tbody class="bg-white divide-y">
                <tr>
                  <td class="px-6 py-4 whitespace-nowrap">
                      <div class="text-sm text-gray-900"><%= Link.last.texto %></div>
                  </td>
                  <td class="px-6 py-4 whitespace-nowrap">
                      <div class="text-sm text-gray-900"><%= Link.last.botao %></div>
                  </td>
                  <td class="px-6 py-4 whitespace-nowrap">
                      <div class="text-sm text-gray-900"><%= Link.last.link %></div>
                  </td>
                  <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
                      <%= link_to 'Editar', edit_linker_path, class: 'text-gray-700 hover:text-gray-800 mr-2' %>
                  </td>
                </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    
    
    </div>
  </div>

